laser "setinitpose x=0.3 y=0.3 th=0"
laser "setinitcov Cx=0.001 Cy=0.001 Cth=0.001"

laser "push t='1.0' cmd='localize'"
laser "push t='1.0' cmd='locateBox'"
wait 1

%ignoreobstacles
%turn 90
%ignoreobstacles
%fwd 1.0
%ignoreobstacles
%turn -90

ignoreobstacles
turn 90

ignoreobstacles
turn -90

laser "locateBox detect"
wait 1